[global_config]
[keybindings]
  go_down = <Alt>KP_Down
  go_left = <Alt>KP_Left
  go_right = <Alt>KP_Right
  go_up = <Alt>KP_Up
  next_tab = <Primary>Right
  prev_tab = <Primary>Left
[layouts]
