    [[[child1]]]
      parent = window0
      type = Terminal
    [[[window0]]]
      parent = ""
      type = Window
[plugins]
[profiles]
  [[default]]
    background_image = None
    scroll_on_output = False
    scrollback_infinite = True
    scrollback_lines = 649
